json('../colors.json')
json('../fonts.json')
@import fonts

body, html
  background-color white
  font c1
  color tapa
  margin 0
  padding 0
  letter-spacing 1px

.container
  max-width 900px
  margin auto

ul
  margin 0
  padding 0
li
  margin 0
  padding 0
  list-style-type none

.clear
  &:before
  &:after
    content: ""
    display: table
  &:after
    clear: both
  zoom: 1 if support-for-ie


