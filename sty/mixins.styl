desktop()
  max-width 1200px
  margin auto

clearfix()
  &:before
  &:after
    content: ""
    display: table
  &:after
    clear: both
  zoom: 1 if support-for-ie

fontface(name)
  @font-face
    font-family name
    src url(fnt/name.otf) format('opentype')
    font-weight normal
    font-style normal
